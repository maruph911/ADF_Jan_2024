{
	"name": "PL_CosmosDB_insert_order",
	"properties": {
		"activities": [
			{
				"name": "Copy orders",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "CosmosDbSqlApiSink",
						"writeBehavior": "insert"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "PurchaseOrderID",
									"type": "String"
								},
								"sink": {
									"path": "$['PurchaseOrderID']"
								}
							},
							{
								"source": {
									"name": "CustomerCode",
									"type": "String"
								},
								"sink": {
									"path": "$['CustomerCode']"
								}
							},
							{
								"source": {
									"name": "ProductCode",
									"type": "String"
								},
								"sink": {
									"path": "$['ProductCode']"
								}
							},
							{
								"source": {
									"name": "EmployeeCode",
									"type": "String"
								},
								"sink": {
									"path": "$['EmployeeCode']"
								}
							},
							{
								"source": {
									"name": "PurchaseOrderDate",
									"type": "String"
								},
								"sink": {
									"path": "$['PurchaseOrderDate']"
								}
							},
							{
								"source": {
									"name": "Quantity",
									"type": "String"
								},
								"sink": {
									"path": "$['Quantity']"
								}
							},
							{
								"source": {
									"name": "UnitPrice",
									"type": "String"
								},
								"sink": {
									"path": "$['UnitPrice']"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "DS_Storage_demobitsadlsgen2_PurchaseOrder",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DS_CosmosDb",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}